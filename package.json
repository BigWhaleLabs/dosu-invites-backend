{
  "name": "dosu-invites-backend",
  "version": "1.0.0",
  "description": "Dosu Invites backend code",
  "main": "dist/server.js",
  "repository": "https://github.com/BigWhaleLabs/dosu-invites-backend",
  "author": "backmeupplz <backmeupplz@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "distribute": "node dist/server.js",
    "develop": "tsc-watch --skipLibCheck --onSuccess 'node dist/server.js'",
    "start-ipfs": "ipfs daemon",
    "build-ts": "tsc --skipLibCheck",
    "lint": "yarn pretty && eslint --max-warnings 0 --ext ts,tsx src",
    "pretty": "prettier --check src",
    "postinstall": "yarn prepare",
    "prepare": "yarn generate-abi-types",
    "generate-abi-types": "typechain --target=ethers-v5 'src/types/abi.json' --out-dir 'src/types/abiTypes'",
    "fix-style": "prettier --write src/ && eslint --fix src/"
  },
  "engines": {
    "node": ">=16.13.1"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@hapi/boom": "^9.1.4",
    "@koa/cors": "^3.1.0",
    "@typegoose/typegoose": "^9.5.0",
    "amala": "^7.0.0",
    "cache-manager": "^3.6.0",
    "ethers": "^5.5.4",
    "ffmpeg-extract-frame": "^1.0.2",
    "ffmpeg-extract-frames": "^2.0.2",
    "fluent-ffmpeg": "^2.1.2",
    "ipfs": "^0.62.0",
    "ipfs-http-client": "^56.0.0",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^10.1.1",
    "mongoose": "^6.1.10"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^9.0.0",
    "@types/cache-manager": "^3.4.2",
    "@types/fluent-ffmpeg": "^2.1.20",
    "@types/koa": "^2.13.4",
    "@types/koa-bodyparser": "^4.3.5",
    "@types/koa-router": "^7.4.4",
    "@types/koa__cors": "^3.1.1",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "dotenv": "^15.0.0",
    "envalid": "^7.2.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-no-relative-import-paths": "^1.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "module-alias": "^2.2.2",
    "prettier": "^2.5.1",
    "source-map-support": "^0.5.21",
    "tsc-watch": "^4.6.0",
    "typechain": "^7.0.0",
    "typescript": "^4.5.5"
  },
  "_moduleAliases": {
    "@": "dist"
  },
  "packageManager": "yarn@3.1.1"
}
